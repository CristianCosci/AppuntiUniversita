$TTL 86400
$ORIGIN 168.192.in-addr.arpa.
@ IN SOA dns.osvaldoindustries.it. root.dmz.osvaldoindustries.it. (
	2018112902 ; serial
	43200 ; refresh
	3600 ; retry after 1 hour
	3600000 ; expire after 1000 hours
	2592000 ; default ttl
